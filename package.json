{
  "name": "regl-gpu-lines",
  "version": "0.0.14",
  "description": "Pure GPU instanced, screen-projected lines for regl",
  "main": "dist/regl-gpu-lines.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rreusser/regl-gpu-lines.git"
  },
  "bugs": {
    "url": "https://github.com/rreusser/regl-gpu-lines/issues"
  },
  "scripts": {
    "start": "node scripts/serve-example.js",
    "build:examples": "for i in examples/*.js; do node scripts/build-example $i; done",
    "build": "npm run bundle:rollup && npm run minify:rollup",
    "bundle:rollup": "mkdir -p dist && rollup -c",
    "minify:rollup": "terser -cm -o dist/regl-gpu-lines.min.js dist/regl-gpu-lines.js",
    "prepublishOnly": "npm run build:examples && npm run bundle:rollup && npm run minify:rollup"
  },
  "keywords": [
    "regl",
    "webgl",
    "lines"
  ],
  "author": "Ricky Reusser",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-json": "^4.1.0",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "budo": "^11.6.4",
    "controls-gui": "^2.0.0",
    "controls-state": "^2.0.0",
    "hyperstream": "^1.2.2",
    "regl": "^2.1.0",
    "rollup": "^2.58.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "simple-html-index": "^1.5.0",
    "string-to-stream": "^3.0.1",
    "tape": "^5.3.1",
    "terser": "^5.9.0"
  }
}
